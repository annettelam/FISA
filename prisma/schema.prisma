datasource db {
  provider = "sqlite"
  url      = env("PRISMA_DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id        Int      @id @default(autoincrement())
  name      String?
  email     String   @unique
  password  String
  isAdmin   Boolean  @default(false)
  image     String?
  createdAt DateTime @default(now())
}
